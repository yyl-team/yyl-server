{
  "version": "1.0.0",
  "main": "output/index.js",
  "types": "output/index.d.ts",
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "extends": [
      "yyl-react"
    ]
  },
  "eslintIgnore": [
    "/node_modules",
    "/output",
    "/test"
  ],
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "none",
    "printWidth": 100
  },
  "scripts": {
    "bootstrap": "yarn install & npm run prettier",
    "eslint": "eslint --ext=jsx,ts,tsx ./",
    "prettier": "prettier --write ./**",
    "dev": "rollup --config ./rollup.config.js",
    "d": "rollup --config ./rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup --config ./rollup.config.js",
    "w": "rollup --watch --config ./rollup.config.js",
    "prepublishOnly": "npm run prettier & npm test & npm run doc",
    "doc": "typedoc src/index.ts --plugin typedoc-plugin-markdown",
    "test": "npm run build & jest ./test/jest/"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-yyl-react": "^0.1.9",
    "jest": "26.6.3",
    "prettier": "^2.0.4",
    "rollup": "^2.38.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "typedoc": "^0.20.20",
    "typedoc-plugin-markdown": "^3.4.5",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/anyproxy": "^4.1.1",
    "@types/connect": "^3.4.34",
    "@types/connect-livereload": "^0.5.31",
    "@types/request-promise": "^4.1.47",
    "@types/serve-favicon": "^2.5.2",
    "@types/serve-index": "^1.7.30",
    "@types/serve-static": "^1.13.9",
    "anyproxy": "4.1.3",
    "chalk": "2.4.1",
    "connect-livereload": "0.6.0",
    "express": "^4.17.1",
    "http-graceful-shutdown": "^3.0.1",
    "http-shutdown": "1.2.1",
    "node-easy-cert": "^1.3.3",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "serve-favicon": "2.5.0",
    "serve-index": "1.9.1",
    "serve-static": "1.13.2",
    "yyl-config-types": "^0.3.19",
    "yyl-fs": "0.3.3",
    "yyl-os": "0.11.4",
    "yyl-util": "2.3.9"
  },
  "name": "yyl-server",
  "description": "yyl server",
  "repository": "https://github.com/jackness1208/yyl-server.git",
  "author": "liudaojie <liudaojie@yy.com>",
  "license": "MIT"
}
